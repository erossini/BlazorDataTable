@if (IncludeSearchButton)
{
    <button class="btn btn-light btn-sm rounded-circle align-baseline">
        <img style="cursor: pointer" height="15" width="15" alt="Search" @onclick="(e) => ApplyFilters()" src="data:image/png;base64," />
    </button>
}

@if (IncludeToggleFilters)
{
    @if (ShowHeaderFilters)
    {
        <button class="btn btn-light btn-sm rounded-circle align-baseline">
            <img style="cursor: pointer" height="15" width="15" alt="Search" @onclick="(e) => ShowHeaderFiltersChangedEvent.InvokeAsync(false)" src="data:image/png;base64," />
        </button>
    }
    else
    {
        <button class="btn btn-light btn-sm rounded-circle align-baseline">
            <img style="cursor: pointer" height="15" width="15" alt="Search" @onclick="(e) => ShowHeaderFiltersChangedEvent.InvokeAsync(true)" src="data:image/png;base64," />
        </button>
    }
}

@code {
    [Parameter]
    public bool IncludeSearchButton { get; set; } = false;

    [Parameter]
    public bool IncludeToggleFilters { get; set; } = false;

    [Parameter]
    public bool ShowHeaderFilters { get; set; } = true;

    [Parameter]
    public Func<Task> ApplyFilters { get; set; }

    [Parameter]
    public EventCallback<bool> ShowHeaderFiltersChangedEvent { get; set; }
}
